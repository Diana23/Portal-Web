package com.cablevision.carga.transformador;

import java.util.Map;

/**
 * Clase para transfomar la informacion antes de ser indexada
 * 
 * @author luishpm
 *
 */
public interface Transformador {
	/**
	 * Metodo para transformar el mapa de datos, este método será llamado
	 * antes de indexar la información.
	 * 
	 * @param datos El mapa con los datos
	 * @param index El indice en la coleccion
	 */
	public void transforma(Map<String, String> datos,int index);
}
