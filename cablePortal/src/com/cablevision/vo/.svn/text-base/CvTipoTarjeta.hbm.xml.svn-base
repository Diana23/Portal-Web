<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
    name="com.cablevision.vo.CvTipoTarjeta" 
    table="CV_TIPO_TARJETA"
    lazy="false"
>
    <id
        name="ttaId"
        type="long"
        column="TTA_ID"
        length="22"
    >
        <generator class="assigned" />
    </id>
    <property
        name="ttaNombre"
        type="string"
        column="TTA_NOMBRE"
        not-null="true"
        length="40"
    />

    <!-- Associations -->

</class>
</hibernate-mapping>
