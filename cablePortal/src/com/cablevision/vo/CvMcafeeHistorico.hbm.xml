<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
    name="com.cablevision.vo.CvMcafeeHistorico" 
    table="CV_MCAFEEHISTORICO"
    schema="PORTALWEB"
    lazy="false"
>
    <id
        name="mcaIdHistorico"
        type="long"
        column="MCA_IDHISTORICO"
        length="22"
    >
        <generator class="sequence"  >
    		<param name="sequence">CV_MCAFEEHISTORICO_SEQ</param>
    	</generator>
    </id>
    <property
        name="mcaAccountNumber"
        type="long"
        column="MCA_ACCOUNT"
        not-null="true"
        length="22"
    />
    <property
        name="mcaCvStatus"
        type="string"
        column="MCA_CVSTATUS"
        not-null="false"
        length="250"
    />
    <property
        name="mcaMotivo"
        type="string"
        column="MCA_MOTIVO"
        not-null="false"
        length="250"
    />
    <property
        name="mcaFechaInicio"
        type="timestamp"
        column="MCA_FECHAINICIO"
        not-null="false"
        length="7"
    />
    <property
        name="mcaFechaFin"
        type="timestamp"
        column="MCA_FECHAFIN"
        not-null="false"
        length="250"
    />
    <property
        name="mcaReference"
        type="string"
        column="MCA_REFERENCE"
        not-null="false"
        length="250"
    />
	<property
        name="mcaTipoProducto"
        type="string"
        column="MCA_TIPOPRODUCTO"
        not-null="false"
        length="20"
    />
    
    
    </class>
</hibernate-mapping>
